{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style":"off",
        "no-console":"off",
        "newline-per-chained-call":"off",
        "arrow-parens":"off",

        "import/named":"off",
        "no-undef":"off",
        "no-debugger":"off",
        "consistent-return":"off",
        "object-curly-newline":"off"

        "import/named":"off"

    }
}